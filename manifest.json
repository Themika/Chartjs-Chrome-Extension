{
  "manifest_version": 2,
  "name": "My Chart Extension",
  "version": "1.0",
  "description": "A Chrome extension that displays a chart",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],  
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"

}